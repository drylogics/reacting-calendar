{
  "name": "reacting-calendar",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "moment-range": "^3.1.1",
    "react": "^16.2.0",
    "react-jss": "^8.3.3",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --ignore *.spec.js --out-dir ./build/es",
    "build:umd:dev": "webpack --config scripts/umd.webpack.config.js",
    "build:umd:prod": "cross-env NODE_ENV=production webpack --config scripts/umd.webpack.config.js",
    "build:polish": "babel-node ./tasks/polish-package.js",
    "build": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:umd:dev && yarn build:umd:prod && yarn build:polish",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/*@(.js|.jsx)\" --watch --watch-extensions jsx",
    "testd": "mocha --inspect-brk --compilers js:babel-core/register --require ./test/test_helper.js \"test/*@(.js|.jsx)\" --watch --watch-extensions jsx"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-preval": "^1.6.2",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "fs-extra": "^5.0.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.10",
    "deep-freeze": "0.0.1",
    "enzyme": "3.3.0",
    "flow-copy-source": "^1.2.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "history": "^4.7.2",
    "mocha": "^5.0.1",
    "prettier": "^1.8.2",
    "react-dom": "^16.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.3.0",
    "webpack": "^3.6.0"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3000",
      "ws": false
    },
    "/auth/google_oauth2": {
      "target": "http://localhost:3000",
      "ws": false
    },
    "/auth/google_oauth2/callback": {
      "target": "http://localhost:3000",
      "ws": false
    },
    "/backend": {
      "target": "http://localhost:3000",
      "ws": false
    }
  },
  "repository": "git@github.com:drylogics/reacting-calendar.git",
  "author": "Vasanth Amana <vasanth@drylogics.com>",
  "license": "MIT"
}
